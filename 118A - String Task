#include <iostream>
using namespace std;

int main()
{
    string s;
    cin>>s;
    string res = "";
    for(int i=0;i<s.size();i++){
        char temp = tolower(s[i]);
        if(temp!='a' and temp!='e'  and  temp!='i' and  temp!='o' and temp!='u' and temp!='y'){
            res += '.';
            res+= temp;
        }
    }
    cout<<res<<endl;
    return 0;
}
