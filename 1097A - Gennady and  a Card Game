#include <iostream>
using namespace std;

int main()
{
    string n;
    cin>>n;
    string arr[5];
    int flag = 0;
    for(int i=0;i<5;i++){
        cin>>arr[i];
    }
    for(int i= 0;i<5;i++){
        string t = arr[i];
        if(t[0]==n[0] or t[1]==n[1]) flag = 1;
    }
    if(flag) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
    return 0;
}
